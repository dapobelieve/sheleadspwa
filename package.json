{
  "name": "sheleads-pwa",
  "version": "1.0.0",
  "scripts": {
    "dev": "vue-cli-service serve",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:local": "vue-cli-service build && serve -s dist/",
    "lint": "vue-cli-service lint",
    "bundlesize": "bundlesize",
    "start": "node server.js"
  },
  "engines": {
    "npm": ">=5.5.0",
    "node": ">=9.3.0"
  },
  "dependencies": {
    "@joeattardi/emoji-button": "^3.0.3",
    "ably": "^1.1.24",
    "axios": "^0.19.2",
    "core-js": "^3.1.3",
    "date-fns": "^2.13.0",
    "express": "^4.17.1",
    "firebase": "^7.15.0",
    "husky": "^4.2.3",
    "is_js": "^0.9.0",
    "lodash": "^4.17.15",
    "pretty-quick": "^2.0.1",
    "pwacompat": "^2.0.9",
    "register-service-worker": "^1.6.2",
    "serve-static": "^1.14.1",
    "vue": "^2.6.10",
    "vue-head": "^2.1.1",
    "vue-moment": "^4.1.0",
    "vue-phone-number-input": "^1.1.9",
    "vue-router": "^3.0.6",
    "vue-toasted": "^1.1.28",
    "vuex": "^3.1.1",
    "vuex-persistedstate": "^3.0.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.132",
    "@vue/cli-plugin-babel": "^4.0.5",
    "@vue/cli-plugin-pwa": "^3.8.0",
    "@vue/cli-service": "^4.0.5",
    "@vue/eslint-config-prettier": "^5.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-lodash": "^3.3.4",
    "bundlesize": "github:kefranabg/bundlesize",
    "chalk": "^2.4.2",
    "change-case": "3.1.0",
    "node-sass": "^4.12.0",
    "prerender-spa-plugin": "^3.4.0",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "shelljs": "^0.8.3",
    "prettier": "1.19.1",
    "vue-template-compiler": "^2.6.10",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-merge": "^4.2.1"
  },
  "prettier": {
    "tabWidth": 2,
    "printWidth": 200,
    "semi": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "bundlesize": [
    {
      "path": "./dist/js/chunk-vendors*.js",
      "maxSize": "125 kB"
    },
    {
      "path": "./dist/js/app*.js",
      "maxSize": "15 kB"
    },
    {
      "path": "./dist/js/client-chunk-*.js",
      "maxSize": "6 kB"
    }
  ],
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  }
}
